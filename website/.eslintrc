{
  "extends": "eslint-config-airbnb",
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": [0],
    "func-names": [0],
    "semi": [2, "never"],
    "no-extra-semi": [2],
    "space-before-function-paren": [2, "always"],
    "no-else-return": [0],
    "space-infix-ops": [0],
    "react/prefer-es6-class": [0],
    "react/prefer-stateless-function": [0],
    "import/no-unresolved": [0],
    "global-require": [0],
  },
  "globals": {
    "__PREFIX_LINKS__": true,
  },
}
